{"query studyDetailViewQuery($lang: languageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"studypage\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeStudypage {\n        title\n        fieldPerson\n        fieldStudyTag {\n          entity {\n            ... on TaxonomyTermStudytag {\n              entityId\n              entityLabel\n            }\n          }\n        }\n        fieldHighlightedStudyIntro\n        fieldIntroduction\n        fieldContent\n        fieldAdditionalImages {\n          derivative(style: CROP_LARGE) {\n            url\n          }\n          alt\n          title\n        }\n        fieldStudyText {\n          url {\n            routed\n            path\n          }\n          title\n        }\n        fieldAccordion {\n          entity {\n            ... on ParagraphStudyPageAccordion {\n              fieldStudyPageAccordionTitle\n              fieldStudyAccordionIntro\n              fieldStudyAccordionContent\n            }\n          }\n        }\n        fieldLabel {\n          entity {\n            ... on TaxonomyTermStudylabel {\n              entityId\n              entityLabel\n            }\n          }\n        }\n        fieldRightColumn {\n          entity {\n            ... on ParagraphStudypageRightColumn {\n              fieldStudy {\n                entity {\n                  ... on study {\n                    fieldAuthor\n                    fieldAuthorInstitution\n                    fieldPublicationLang {\n                      entity {\n                        ... on TaxonomyTermPublicationlanguage {\n                          entityId\n                          entityLabel\n                        }\n                      }\n                    }\n                    fieldYear\n                    fieldOrderer\n                    fieldPublisher\n                    fieldPublicationType {\n                      entity {\n                        ... on TaxonomyTermPublicationtype {\n                          entityId\n                          entityLabel\n                        }\n                      }\n                    }\n                  }\n                }\n              }\n              fieldAdditionalLinks {\n                entity {\n                  ... on ParagraphAdditionalLinks {\n                    fieldLinks {\n                      url {\n                        routed\n                        path\n                      }\n                      title\n                    }\n                  }\n                }\n              }\n            }\n          }\n        }\n      }\n    }\n  }\n}\n":1}