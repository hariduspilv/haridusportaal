{"query careerPage($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"career_home_page\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeCareerHomePage {\n        fieldContentPageLink {\n          entity {\n            ... on ParagraphContentPageLink {\n              fieldTheme\n              fieldTitle\n              fieldText\n              fieldInternalLink {\n                entity {\n                  entityLabel\n                  entityUrl {\n                    path\n                    routed\n                  }\n                }\n              }\n            }\n          }\n        }\n        fieldCareer {\n          entity {\n            entityLabel\n            entityUrl {\n              path\n              routed\n            }\n          }\n        }\n        fieldFrontpageNews {\n          entity {\n            ... on NodeNews {\n              entityLabel\n              entityUrl {\n                path\n                routed\n              }\n            }\n          }\n        }\n        fieldQuoteText\n        fieldQuoteAuthor\n        fieldQuoteAuthorOccupation\n        fieldCareerContact {\n          entity {\n            ... on ParagraphCareerContact {\n              fieldInstitution\n              fieldNameOccupation\n              fieldEmail\n              fieldSkype\n            }\n          }\n        }\n        fieldExternalLinks {\n          entity {\n            ... on ParagraphLink {\n              fieldWebpageLink {\n                url {\n                  routed\n                  path\n                }\n              }\n              fieldLinkName\n            }\n          }\n        }\n      }\n    }\n  }\n}\n":1}