{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-dom": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1",
        "drupal/admin_toolbar": "^3.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/cache_tags": "^1.0@beta",
        "drupal/computed_field": "^2.0",
        "drupal/conditional_fields": "^4.0",
        "drupal/config_sync": "^2.0",
        "drupal/console": "~1.0",
        "drupal/console-extend-plugin": "^0.9.5",
        "drupal/core": "9.2.7 as 8.9.17",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.2",
        "drupal/elasticsearch_connector": "^7.0",
        "drupal/elasticsearch_connector_autocomp": "1.x-dev",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/externalauth": "^1.3",
        "drupal/field_defaults": "^1.3",
        "drupal/google_map_field": "^1.5",
        "drupal/graphql": "^3.1",
        "drupal/graphql_metatag": "^1.0",
        "drupal/hms_field": "^1.0@beta",
        "drupal/image_widget_crop": "^2.1",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_autocomplete": "^1.1",
        "drupal/json_feed": "^1.1",
        "drupal/jwt": "^1.0@alpha",
        "drupal/material_admin": "^2.0",
        "drupal/maxlength": "^1.0@beta",
        "drupal/memcache": "^2.3",
        "drupal/menu_breadcrumb": "^1.6",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/metatag": "^1.16",
        "drupal/module_missing_message_fixer": "^2.0",
        "drupal/monolog": "^2.0.0-beta2",
        "drupal/node_view_permissions": "^1.2",
        "drupal/openid_connect": "^1.0",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.1",
        "drupal/queue_ui": "^2.2",
        "drupal/readonly_field_widget": "^1.2",
        "drupal/redirect": "^1.5",
        "drupal/redis": "^1.0",
        "drupal/restui": "^1.15",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.19",
        "drupal/seckit": "^2.0",
        "drupal/session_limit": "^2.0@beta",
        "drupal/sitemap": "^2.0-beta2",
        "drupal/smtp": "^1.0",
        "drupal/svg_image": "^1.9",
        "drupal/svg_image_field": "^1.2",
        "drupal/taxonomy_multidelete_terms": "^1.1",
        "drupal/ultimate_cron": "^2.0",
        "drush/drush": "^10.4",
        "elasticsearch/elasticsearch": "^7.0",
        "eluceo/ical": "^0.14.0",
        "hshn/base64-encoded-file": "^v3.0.0",
        "jumbojett/openid-connect-php": "^0.7.0",
        "justinrainbow/json-schema": "^5.2",
        "kint-php/kint": "^3.3",
        "league/csv": "^9.0",
        "ralouphie/mimey": "^2.0",
        "ruflin/elastica": "dev-master",
        "swaggest/json-schema": "^0.12.7",
        "symfony/dom-crawler": "^4.4 !=4.4.5",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "webonyx/graphql-php": "^0.12.5"
    },
    "require-dev": {
        "drupal/core-dev": "^9"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "/web"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/core": {
                "AccountProxy Drupal 9 custom patch": "web/patches/AccountProxy_D9.txt"
            },
            "drupal/material_admin": {
                "Material Admin Fixing Array to String Notice for PHP 7 on 2.X": "https://www.drupal.org/files/issues/2021-02-08/material_admin-v2-array_to_string_notice.patch",
                "Drupal 9 deprecated code fix": "https://www.drupal.org/files/issues/2021-06-17/deprecated-3075746-3.patch",
                "Drupal 9 autocomplete fix": "web/patches/mat_admin_autocomplete_D9.txt"
            },
            "drupal/cache_tags": {
                "Cache Tags Drupal 9 patch": "https://www.drupal.org/files/issues/2021-01-19/cache_tags-3139751.patch"
            },
            "drupal/hms_field": {
                "HMS Field Drupal 9 patch": "https://www.drupal.org/files/issues/2021-05-09/3147750-d9-compatibility-2.patch"
            },
            "drupal/menu_item_role_access": {
                "Call to a member function hasField() on null": "https://www.drupal.org/files/issues/2021-02-09/hasField-on-null-3197539-2.patch"
            }
        }
    }
}
