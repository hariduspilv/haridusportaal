{
    "name": "hshn/base64-encoded-file",
    "description": "Provides handling base64 encoded files, and the integration of symfony/form",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Hoshino",
            "email": "lga0503@gmail.com"
        }
    ],
    "require": {
        "PHP": ">=5.4.0",
        "symfony/http-foundation": "~2.7|~3.0"
    },
    "require-dev": {
        "symfony/form": "~2.7|~3.0"
    },
    "suggest": {
        "symfony/form": "to use base64_encoded_file type"
    },
    "autoload": {
        "psr-4": { "Hshn\\Base64EncodedFile\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Hshn\\Base64EncodedFile\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
