/**
 * hook_cron()
 */
function htm_custom_kafka_module_cron(){
	\Drupal::state()->set('htm_custom_kafka_module_cron', time());
	//$action = \Drupal::state()->get('ultimate_cron_logger_test_cron_action');
	//kint($action);
	$kafka_service = \Drupal::service('htm_custom_kafka_module.default');
	$kafka_service->ConsumeNewMessages('logs');
}