{"query oskaResultPageDetailView($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"oska_result_page\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeOskaResultPage {\n        nid\n        entityLabel\n        fieldResultPageIntroduction\n        body {\n          value\n        }\n        fieldResultPageSidebar {\n          entity {\n            ... on ParagraphRightSidebarSectionInfo {\n              fieldAdditional {\n                entity {\n                  ... on ParagraphAdditionalSection {\n                    fieldTitle\n                    fieldAdditionalBody\n                  }\n                }\n              }\n              fieldContactSection {\n                entity {\n                  ... on ParagraphContactSection {\n                    fieldPhone\n                    fieldPerson\n                    fieldEmail\n                    fieldOrganization\n                  }\n                }\n              }\n              fieldRelatedArticle {\n                url {\n                  path\n                }\n                title\n              }\n              fieldHyperlinks {\n                url {\n                  path\n                }\n                title\n              }\n            }\n          }\n        }\n        fieldResultPageVideo {\n          input\n          videoDomain\n          videoDescription\n          videoId\n        }\n        fieldResultPageLinks {\n          url {\n            path\n            __typename\n          }\n          title\n        }\n        fieldResultPageFiles {\n          description\n          entity {\n            entityLabel\n            entityType\n            entityChanged\n            entityBundle\n            entityCreated\n            url\n            filename\n            filemime\n            __typename\n          }\n          __typename\n        }\n        fieldResultPageTableFile {\n          description\n          entity {\n            entityLabel\n            entityType\n            entityChanged\n            entityBundle\n            entityCreated\n            url\n            filename\n            filemime\n            __typename\n          }\n          __typename\n        }\n      }\n    }\n  }\n}\n":1}