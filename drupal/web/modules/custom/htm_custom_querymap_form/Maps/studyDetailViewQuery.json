g {\n                        entity {\n                          ... on TaxonomyTermPublicationlanguage {\n                            entityId\n                            entityLabel\n                          }\n                        }\n                      }\n                      fieldYear\n                      fieldOrderer\n                      fieldPublisher\n                      fieldPublicationType {\n                        entity {\n                          ... on TaxonomyTermPublicationtype {\n                            entityId\n                            entityLabel\n                          }\n                        }\n                      }\n                    }\n                  }\n                }\n                fieldAdditionalLinks {\n                  entity {\n                    ... on ParagraphAdditionalLinks {\n                      fieldLinks {\n                        url {\n                          routed\n                          path\n                        }\n                        title\n                      }\n                    }\n                  }\n                }\n              }\n            }\n          }\n        }\n      }\n    }\n  }\n}\n": 1
