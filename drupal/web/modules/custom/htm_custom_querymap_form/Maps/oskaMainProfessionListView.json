{"query oskaMainProfessionListView($nid: [String], $nidEnabled: Boolean, $titleValue: String, $titleEnabled: Boolean, $fixedLabelValue: [String], $fixedLabelEnabled: Boolean, $oskaFieldValue: [String], $oskaFieldEnabled: Boolean, $lang: LanguageId!, $offset: Int, $limit: Int, $sortField: String!, $sortDirection: SortOrder, $indicatorSort: Boolean, $fillingBarValues: [String], $fillingBarFilterEnabled: Boolean, $fieldProfession: [String], $fieldProfessionEnabled: Boolean) {\n  nodeQuery(offset: $offset, limit: $limit, sort: {field: $sortField, direction: $sortDirection, language: $lang}, filter: {conjunction: AND, conditions: [{operator: EQUAL, field: \"status\", value: \"1\", language: $lang}, {operator: EQUAL, field: \"field_profession\", value: $fieldProfession, enabled: $fieldProfessionEnabled, language: $lang}, {operator: IN, field: \"field_filling_bar\", value: $fillingBarValues, enabled: $fillingBarFilterEnabled}, {operator: GREATER_THAN, field: $sortField, value: \"0\", enabled: $indicatorSort}, {operator: EQUAL, field: \"type\", value: [\"oska_main_profession_page\"], language: $lang}, {operator: IN, field: \"nid\", value: $nid, language: $lang, enabled: $nidEnabled}, {operator: IN, field: \"field_fixed_label\", value: $fixedLabelValue, language: $lang, enabled: $fixedLabelEnabled}, {operator: IN, field: \"field_sidebar.entity.field_oska_field\", value: $oskaFieldValue, language: $lang, enabled: $oskaFieldEnabled}], groups: [{conjunction: OR, conditions: [{operator: LIKE, field: \"title\", value: [$titleValue], language: $lang, enabled: $titleEnabled}, {operator: LIKE, field: \"field_sidebar.entity.field_jobs.entity.field_job_name\", value: [$titleValue], language: $lang, enabled: $titleEnabled}]}]}) {\n    count\n    entities(language: $lang) {\n      ... on NodeOskaMainProfessionPage {\n        nid\n        title\n        fieldProfession\n        fieldFurtherInfo {\n          value\n          processed\n        }\n        fieldFixedLabel {\n          entity {\n            ... on TaxonomyTermOskaFixedLabels {\n              entityId\n              entityLabel\n            }\n          }\n        }\n        entityUrl {\n          path\n        }\n        fieldSidebar {\n          entity {\n            ... on ParagraphMainProfessionSidebar {\n              fieldPros\n              fieldNeutral\n              fieldCons\n              fieldOskaField {\n                entity {\n                  ... on NodeOskaFieldPage {\n                    nid\n                    title\n                    entityUrl {\n                      path\n                    }\n                  }\n                }\n              }\n            }\n          }\n        }\n        reverseOskaMainProfessionOskaIndicatorEntity {\n          entities {\n            ... on OskaIndicatorEntity {\n              oskaId\n              oskaIndicator\n              value\n              icon\n            }\n          }\n        }\n        reverseOskaMainProfessionOskaFillingBarEntity {\n          entities {\n            ... on OskaFillingBarEntity {\n              value\n            }\n          }\n        }\n        __typename\n        fieldBruto\n        fieldEducationIndicator\n        fieldNumberOfEmployees\n        fieldChangeInEmployment\n        fieldFillingBar\n      }\n      __typename\n    }\n    __typename\n  }\n  comparisonPage: nodeQuery(limit: 1, sort: {field: \"changed\", direction: DESC}, filter: {conditions: [{field: \"type\", value: \"main_profession_comparison_page\", language: $lang}, {field: \"status\", value: \"1\"}]}) {\n    count\n  }\n}\n":1}