{"query frontPageQuery($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"avaleht\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeAvaleht {\n        fieldHeadBlockTitle\n        fieldTopics {\n          entity {\n            ... on ParagraphHomepageTopics {\n              fieldTopicTitle\n              fieldTopicText\n              fieldTopicLink {\n                url {\n                  routed\n                  path\n                }\n              }\n            }\n          }\n        }\n        fieldSupernews {\n          entity {\n            ... on ParagraphSupernews {\n              fieldSupernewsNode {\n                entity {\n                  ... on NodeNews {\n                    nid\n                    title\n                    entityUrl {\n                      routed\n                      path\n                    }\n                    fieldAuthor\n                    created\n                    fieldIntroductionImage {\n                      derivative(style: CROP_LARGE) {\n                        url\n                      }\n                      targetId\n                      alt\n                      title\n                    }\n                  }\n                }\n              }\n              fieldPublishDate {\n                value\n                date\n                unix\n              }\n              fieldUnpublishDate {\n                value\n                date\n                unix\n              }\n            }\n          }\n        }\n      }\n    }\n  }\n}\n":1}