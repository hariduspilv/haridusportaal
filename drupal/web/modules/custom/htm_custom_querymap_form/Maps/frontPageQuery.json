{"query frontPageQuery($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"avaleht\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeAvaleht {\n        fieldHeadBlockTitle\n        fieldTopics {\n          entity {\n            ... on ParagraphHomepageTopics {\n              fieldTopicTitle\n              fieldTopicText\n              fieldTopicLink {\n                url {\n                  routed\n                  path\n                }\n              }\n            }\n          }\n        }\n        fieldSupernews {\n          entity {\n            ... on NodeNews {\n              nid\n              title\n              entityUrl {\n                routed\n                path\n              }\n              fieldAuthor\n              created\n            }\n          }\n        }\n        fieldSupernewsPublishDate {\n          value\n          date\n          unix\n        }\n        fieldSupernewsUnpublishDate {\n          value\n          date\n          unix\n        }\n        fieldSecondarySupernews {\n          entity {\n            ... on NodeNews {\n              nid\n              title\n              entityUrl {\n                routed\n                path\n              }\n              fieldAuthor\n              created\n            }\n          }\n        }\n        fieldSecondarySupernewsPub {\n          value\n          date\n          unix\n        }\n        fieldSecondarySupernewsUnpub {\n          value\n          date\n          unix\n        }\n      }\n    }\n  }\n}\n":1}