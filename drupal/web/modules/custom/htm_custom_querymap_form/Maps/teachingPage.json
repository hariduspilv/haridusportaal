{"query teachingPage($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"changed\", direction: DESC}, filter: {conditions: {field: \"type\", value: \"teaching_home_page\", language: $lang}}) {\n    entities(language: $lang) {\n      ... on NodeTeachingHomePage {\n        fieldTeachingThemes {\n          entity {\n            ... on ParagraphTeachingThemes {\n              fieldThemeTitle\n              fieldInternalLink {\n                entity {\n                  entityUrl {\n                    routed\n                    path\n                  }\n                }\n              }\n            }\n          }\n        }\n        fieldTeachingNews {\n          entity {\n            ... on NodeNews {\n              entityUrl {\n                path\n                routed\n              }\n              title\n              fieldShortDescription\n            }\n          }\n        }\n        fieldToolbox {\n          entity {\n            ... on ParagraphToolbox {\n              fieldTitle\n              fieldContent\n              fieldInternalLink {\n                entity {\n                  entityUrl {\n                    routed\n                    path\n                  }\n                }\n              }\n              fieldLinkName\n              fieldToolboxImage {\n                entity {\n                  ... on File {\n                    url\n                  }\n                }\n              }\n            }\n          }\n        }\n        fieldQuoteText\n        fieldQuoteAuthor\n        fieldQuoteAuthorOccupation\n        fieldContact {\n          entity {\n            ... on ParagraphCareerContact {\n              fieldSkype\n              fieldEmail\n              fieldInstitution\n              fieldNameOccupation\n            }\n          }\n        }\n        fieldExternal {\n          entity {\n            ... on ParagraphLink {\n              fieldLinkName\n              fieldWebpageLink {\n                url {\n                  path\n                }\n              }\n            }\n          }\n        }\n      }\n    }\n  }\n}\n":1}