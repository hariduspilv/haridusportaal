{"query ($lang: LanguageId!) {\n  nodeQuery(sort: {field: \"created\", direction: DESC}, filter: {conditions: [{operator: EQUAL, field: \"type\", value: [\"youth_monitoring_page\"], language: $lang}]}) {\n    entities(language: $lang) {\n      ... on NodeYouthMonitoringPage {\n        entityLabel\n        created\n        entityUrl {\n          ... on EntityCanonicalUrl {\n            path\n            languageSwitchLinks {\n              url {\n                path\n                routed\n                pathAlias\n                pathInternal\n              }\n            }\n          }\n        }\n        title\n        fieldIntroduction\n        fieldExtertnalLink {\n          entity {\n            ... on ParagraphLink {\n              fieldLinkName\n              fieldWebpageLink {\n                title\n                uri\n              }\n            }\n          }\n        }\n        fieldFirstPictureOrVideo\n        fieldFirstPicture {\n          url\n          title\n          alt\n          title\n        }\n        fieldFirstVideo {\n          input\n          videoDomain\n          videoDescription\n          videoId\n          videoThumbnail\n        }\n      }\n    }\n  }\n}\n":1}