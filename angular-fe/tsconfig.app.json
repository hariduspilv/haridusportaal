{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"],
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/environments/*.ts",
    "src/app/_views/frontpageView/*.ts",
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/app/_modules/settings/settings.module.ts"
  ]
}
