  <complexType name="TranscriptOfGradesIssuer">
    <sequence>
      <element name="IssuerNameToPrintout" type="string"/>
    </sequence>
  </complexType>

  <complexType name="StudySubjects">
    <sequence>
      <element maxOccurs="unbounded" name="StudySubject" type="tns:StudySubject"/>
    </sequence>
  </complexType>

  <complexType name="StudySubject">
    <sequence>
      <element name="Type">
        <simpleType>
          <restriction base="string">
            <enumeration value="STUDY_SUBJECT_TYPE:MANDATORY"/>
            <enumeration value="STUDY_SUBJECT_TYPE:OPTIONAL"/>
          </restriction>
        </simpleType>
      </element>
      <element name="ItemNumber" type="integer"/>
      <element name="Subject" type="string"/>
      <element minOccurs="0" name="SubjectDetail" type="string"/>
      <element minOccurs="0" name="ResultNumeric" type="integer"/>
      <element minOccurs="0" name="ResultInWords" type="string"/>
      <element minOccurs="0" name="Amount" type="integer"/>
      <element minOccurs="0" name="StudyProgrammeType" type="string"/>
    </sequence>
  </complexType>

  <complexType name="GraduationStudy">
    <sequence>
      <element name="Type">
        <simpleType>
          <restriction base="string">
            <enumeration value="GRADUATION_STUDY_TYPE:RESEARCH"/>
            <enumeration value="GRADUATION_STUDY_TYPE:PRACTICAL_WORK"/>
            <enumeration value="GRADUATION_STUDY_TYPE:CREATIVE_WORK"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Subject" type="string"/>
      <element minOccurs="0" name="ResultNumeric" type="integer"/>
      <element minOccurs="0" name="ResultInWords" type="string"/>
    </sequence>
  </complexType>

  <complexType name="GraduationExaminations">
    <sequence>
      <element maxOccurs="unbounded" name="GraduationExamination" type="tns:GraduationExamination"/>
    </sequence>
  </complexType>

  <complexType name="GraduationExamination">
    <sequence>
      <element name="ItemNumber" type="integer"/>
      <element name="Subject" type="string"/>
      <element minOccurs="0" name="ResultNumeric" type="integer"/>
      <element minOccurs="0" name="ResultInWords" type="string"/>
      <element minOccurs="0" name="Type">
        <simpleType>
          <restriction base="string">
            <enumeration value="GRADUATION_EXAMINATION_TYPE:SCHOOL_EXAM"/>
            <enumeration value="GRADUATION_EXAMINATION_TYPE:FINAL_EXAM"/>
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>

  <complexType name="ConductGrade">
    <sequence>
      <element name="ResultInWords" type="string"/>
    </sequence>
  </complexType>

  <!-- ServiceErrors -->
  <complexType name="ServiceErrors">
    <sequence>
      <element maxOccurs="unbounded" name="ServiceError" type="tns:ServiceError"/>
    </sequence>
  </complexType>

  <complexType name="ServiceError">
    <sequence>
      <element minOccurs="0" name="Object" type="string"/>
      <element minOccurs="0" name="Field" type="string"/>
      <element minOccurs="0" name="Value"/>
      <element minOccurs="0" name="Code" type="string"/>
      <element minOccurs="0" name="Message" type="string"/>
    </sequence>
  </complexType>

  <!-- PostCertificateIndexResponseBodyType -->
  <complexType name="PostCertificateIndexResponseBody">
    <sequence>
      <element name="Index" type="tns:CertificateIndex"/>
      <element name="Actions" type="tns:CertificateActions"/>
      <element name="Documents" type="tns:CertificateDocuments"/>
    </sequence>
  </complexType>

  <complexType name="CertificateIndex">
    <sequence>
      <element name="Id" type="long"/>
      <element minOccurs="0" name="ValidFrom" type="date"/>
      <element minOccurs="0" name="ValidUntil" type="date"/>
      <element minOccurs="0" name="ClassStudyGroup" type="string"/>
      <element name="Number" type="string"/>
      <element minOccurs="0" name="OwnerId" type="string"/>
      <element name="OwnerIdCode" type="string"/>
      <element name="OwnerName" type="string"/>
      <element minOccurs="0" name="OwnerDateOfBirth" type="date"/>
      <element name="Status" type="string"/>
      <element name="Type" type="string"/>
      <element name="TypeName" type="string"/>
      <element name="Issued" type="date"/>
      <element minOccurs="0" name="IssuerId" type="string"/>
      <element minOccurs="0" name="IssuerName" type="string"/>
      <element name="IssuerRegCode" type="string"/>
      <element minOccurs="0" name="Curriculum" type="string"/>
      <element minOccurs="0" name="CurriculumName" type="string"/>
    </sequence>
  </complexType>

  <complexType name="CertificateActions">
    <sequence>
      <element maxOccurs="unbounded" name="Action" type="tns:CertificateAction"/>
    </sequence>
  </complexType>

  <complexType name="CertificateAction">
    <sequence>
      <element name="Id" type="long"/>
      <element name="Type" type="string"/>
      <element minOccurs="0" name="Status" type="string"/>
      <element minOccurs="0" name="Added" type="dateTime"/>
      <element minOccurs="0" name="Approved" type="dateTime"/>
      <element minOccurs="0" name="DecisionNumber" type="string"/>
      <element minOccurs="0" name="DateOfDecision" type="date"/>
      <element minOccurs="0" name="Reason" type="string"/>
      <element minOccurs="0" name="Explanation" type="string"/>
      <element minOccurs="0" name="NotificationEmail" type="string"/>
    </sequence>
  </complexType>

  <complexType name="CertificateDocuments">
    <sequence>
      <element maxOccurs="unbounded" name="Document" type="tns:CertificateDocument"/>
    </sequence>
  </complexType>

  <complexType name="CertificateDocument">
    <sequence>
      <element name="Id" type="long"/>
      <element name="Type" type="string"/>
      <element name="TypeName" type="string"/>
      <element name="Revision" type="integer"/>
      <element name="Language" type="string"/>
      <element name="Status" type="string"/>
      <element minOccurs="0" name="Number" type="string"/>
    </sequence>
  </complexType>

  <!-- Grants -->
  <element name="getGrantsRequest">
    <complexType>
      <sequence>
        <element name="personIdCode" type="string">
          <annotation>
            <appinfo>
              <xrd:title>Taotleja isikukood</xrd:title>
            </appinfo>
          </annotation>
        </element>
        <element minOccurs="0" name="grantNumber" type="string">
          <annotation>
            <appinfo>
              <xrd:title>Toetuse number</xrd:title>
              <xrd:techNotes>Kui täidetud tagastab konkreetse toetuse andmestiku</xrd:techNotes>
            </appinfo>
          </annotation>
        </element>
        <element minOccurs="0" name="grantType">
          <annotation>
            <appinfo>
              <xrd:title>Toetuse liik</xrd:title>
              <xrd:techNotes>Lähtetoetuse korral, võtab vastu ühe kahest väärtusest
                'GRANT_TYPE:STARTING_ALLOWANCE_PEDAGOGUE',
                'GRANT_TYPE:STARTING_ALLOWANCE_SPECIALIST'.
              </xrd:techNotes>
            </appinfo>
          </annotation>
          <simpleType>
            <restriction base="string">
              <enumeration value="GRANT_TYPE:STARTING_ALLOWANCE_PEDAGOGUE"/>
              <enumeration value="GRANT_TYPE:STARTING_ALLOWANCE_SPECIALIST"/>
            </restriction>
          </simpleType>
        </element>
        <element minOccurs="0" name="action">
          <annotation>
            <appinfo>
              <xrd:title>Teenuse tegevused</xrd:title>
              <xrd:techNotes>nill/puudub - nimekirja päring, 'hasAccessControl' - õiguste
                kontroll, 'getApplicationData' - alusandmete päring, 'getGrant' - üksikpäring
              </xrd:techNotes>
            </appinfo>
          </annotation>
          <simpleType>
            <restriction base="string">
              <enumeration value="hasAccessControl"/>
              <enumeration value="getApplicationData"/>
              <enumeration value="getGrant"/>
            </restriction>
          </simpleType>
        </element>
      </sequence>
    </complexType>
  </element>
  <element name="getGrantsResponse">
    <complexType>
      <sequence>
        <element minOccurs="0" name="responseInfo" type="tns:responseInfo"/>
        <element minOccurs="0" name="grants" type="tns:grants"/>
        <element minOccurs="0" name="fault" type="tns:ServiceErrors"/>
      </sequence>
    </complexType>
  </element>

  <element name="postGrantsRequest">
    <complexType>
      <sequence>
        <element name="grant" type="tns:grant"/>
      </sequence>
    </complexType>
  </element>
  <element name="postGrantsResponse">
    <complexType>
      <sequence>
        <element minOccurs="0" name="responseInfo" type="tns:responseInfo"/>
        <element minOccurs="0" name="grant" type="tns:grant"/>
        <element minOccurs="0" name="fault" type="tns:ServiceErrors"/>
      </sequence>
    </complexType>
  </element>

