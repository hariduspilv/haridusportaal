      <element minOccurs="0" name="Language" type="string"/>
      <element minOccurs="0" name="ClassStudyGroup" type="string"/>
    </sequence>
  </complexType>

  <complexType name="CertificateCouncilDecision">
    <sequence>
      <element name="DecisionNumber" type="string"/>
      <element name="DateOfDecision" type="date"/>
      <element name="DateOfDecisionToPrintOut" type="string"/>
    </sequence>
  </complexType>

  <complexType name="TranscriptOfGrades">
    <sequence>
      <element name="Number" type="string"/>
      <element name="Language" type="string"/>
      <element name="Issuer" type="tns:TranscriptOfGradesIssuer"/>
      <element minOccurs="0" name="StudySubjects" type="tns:StudySubjects"/>
      <element minOccurs="0" name="GraduationStudy" type="tns:GraduationStudy"/>
      <element minOccurs="0" name="GraduationExaminations" type="tns:GraduationExaminations"/>
      <element minOccurs="0" name="ConductGrade" type="tns:ConductGrade"/>
      <element minOccurs="0" name="IssueDate" type="dateTime"/>
    </sequence>
