<?xml version="1.0" encoding="UTF-8" standalone="no"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch="http://ehis2.x-road.eu" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://ehis2.x-road.eu" xmlns:xrd="http://x-road.eu/xsd/xroad.xsd" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" targetNamespace="http://ehis2.x-road.eu">
  <wsdl:types>
    <schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ref="http://ws-i.org/profiles/basic/1.1/xsd" targetNamespace="http://ehis2.x-road.eu">
  <import namespace="http://x-road.eu/xsd/xroad.xsd" schemaLocation="http://x-road.eu/xsd/xroad.xsd"/>
  <import namespace="http://x-road.eu/xsd/xroad.xsd" schemaLocation="http://x-road.eu/xsd/xroad.xsd"/>
  <import namespace="http://ws-i.org/profiles/basic/1.1/xsd" schemaLocation="http://ws-i.org/profiles/basic/1.1/swaref.xsd"/>

  <!-- Certificates -->
  <element name="SendCertificateRequest" type="tns:SendCertificateRequest"/>
  <element name="SendCertificateResponse">
    <complexType>
      <choice>
        <element name="ServiceErrors" type="tns:ServiceErrors"/>
        <element name="Certificate" type="tns:PostCertificateIndexResponseBody"/>
      </choice>
    </complexType>
  </element>

  <!-- SendCertificateRequestType -->
  <complexType name="SendCertificateRequest">
    <sequence>
      <element name="ProcessType">
        <simpleType>
          <restriction base="string">
            <enumeration value="APPLICATION"/>
            <enumeration value="SCHEDULER"/>
            <enumeration value="MIGRATION"/>
          </restriction>
        </simpleType>
      </element>
      <element name="Action" type="tns:Action"/>
      <element name="Certificate" type="tns:Certificate"/>
      <element minOccurs="0" name="TranscriptOfGrades" type="tns:TranscriptOfGrades"/>
    </sequence>
  </complexType>

  <complexType name="Action">
    <sequence>
      <element name="ActionType">
        <simpleType>
          <restriction base="string">
            <enumeration value="ACTION_TYPE:ISSUING"/>
            <enumeration value="ACTION_TYPE:ISSUING_DUPLICATE"/>
          </restriction>
        </simpleType>
      </element>
      <element name="DocumentType">
        <simpleType>
          <restriction base="string">
            <enumeration value="GRADUATION_DOCUMENT_TYPE:BASIC_EDUCATION_CERTIFICATE"/>
            <enumeration value="GRADUATION_DOCUMENT_TYPE:GENERAL_EDUCATION_CERTIFICATE"/>
            <enumeration value="GRADUATION_DOCUMENT_TYPE:BASIC_EDUCATION_TRANSCRIPT_OF_GRADES"/>
            <enumeration value="GRADUATION_DOCUMENT_TYPE:GENERAL_EDUCATION_TRANSCRIPT_OF_GRADES"/>
          </restriction>
        </simpleType>
      </element>
      <element name="Status">
        <simpleType>
          <restriction base="string">
            <enumeration value="ACTION_STATUS:CONFIRMED"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="Reason" type="string"/>
      <element minOccurs="0" name="Explanation" type="string"/>
      <element minOccurs="0" name="NotificationEmail" type="string"/>
    </sequence>
  </complexType>

  <complexType name="Certificate">
    <sequence>
      <element name="Type">
        <simpleType>
          <restriction base="string">
            <enumeration value="GRADUATION_DOCUMENT_TYPE:BASIC_EDUCATION_CERTIFICATE"/>
            <enumeration value="GRADUATION_DOCUMENT_TYPE:GENERAL_EDUCATION_CERTIFICATE"/>
          </restriction>
        </simpleType>
      </element>
      <element name="Language" type="string"/>
      <element name="Number" type="string"/>
      <element minOccurs="0" name="Qualification" type="string"/>
      <element minOccurs="0" name="QualificationToPrintOut" type="string"/>
      <element minOccurs="0" name="OriginalCertificateNumber" type="string"/>
      <element name="Owner" type="tns:CertificateOwner"/>
      <element name="Issuer" type="tns:CertificateIssuer"/>
      <element minOccurs="0" name="Studies" type="tns:CertificateStudies"/>
      <element name="CouncilDecision" type="tns:CertificateCouncilDecision"/>
      <element minOccurs="0" name="IssueDate" type="dateTime"/>
    </sequence>
  </complexType>

  <complexType name="CertificateOwner">
    <sequence>
      <element minOccurs="0" name="OwnerId" type="string"/>
      <element name="OwnerIdCode" type="string"/>
      <element name="OwnerFirstName" type="string"/>
      <element name="OwnerLastName" type="string"/>
      <element minOccurs="0" name="OwnerDateOfBirth" type="date"/>
    </sequence>
  </complexType>

  <complexType name="CertificateIssuer">
    <sequence>
      <element name="IssuerId" type="string"/>
      <element name="IssuerName" type="string"/>
      <element name="IssuerNameToPrintout" type="string"/>
      <element minOccurs="0" name="IssuerLocation" type="string"/>
      <element minOccurs="0" name="IssuerLocationToPrintout" type="string"/>
      <element name="IssuerRegCode" type="string"/>
    </sequence>
  </complexType>

  <complexType name="CertificateStudies">
    <sequence>
      <element name="Curriculum" type="string"/>
      <element name="CurriculumName" type="string"/>
      <element minOccurs="0" name="CurriculumNameToPrintOut" type="string"/>
